
<vmi1:analysisDesc
    xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:vmi1="http://anathema.flux.utah.edu/schema/vmi/1">

  <vmi1:name>PPM</vmi1:name>
  <vmi1:description>Pingpong Monitor: Printout value of valuables</vmi1:description>
  <vmi1:author>Richard Li</vmi1:author>
  <vmi1:authorContact>licai@cs.utah.edu</vmi1:authorContact>

  <vmi1:binary>pingpongmonitor</vmi1:binary>

  <vmi1:requiresWrite>true</vmi1:requiresWrite>
  <vmi1:requiresControl>true</vmi1:requiresControl>
  <vmi1:generatesAnnotations>false</vmi1:generatesAnnotations>
  <vmi1:reportsIntermediateResults>true</vmi1:reportsIntermediateResults>
  <vmi1:reportsFinalResults>true</vmi1:reportsFinalResults>
  <vmi1:supportsExternalControl>false</vmi1:supportsExternalControl>
  <vmi1:supportsAutoparseSimpleResults>true</vmi1:supportsAutoparseSimpleResults>
  <vmi1:inParams>

    <vmi1:param isCommandLine="false" isCommandLineSwitched="true">
      <vmi1:name>hypervisor-ignores-userspace-exceptions</vmi1:name>
      <vmi1:longName>hypervisor-ignores-userspace-exceptions</vmi1:longName>
      <vmi1:description>This is what required</vmi1:description>
    </vmi1:param>

    <vmi1:param isCommandLine="true" isCommandLineSwitched="false">
      <vmi1:name>cli_in</vmi1:name>
      <vmi1:description>The input values as of the multi-base in the way clicheck does</vmi1:description>
    </vmi1:param>

  </vmi1:inParams>
  <vmi1:outParams>
  </vmi1:outParams>

</vmi1:analysisDesc>
